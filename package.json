{
	"name": "node",
	"version": "1.0.0",
	"description": "Server app made using Node.js / Express / Typescript",
	"main": "index.js",
	"scripts": {
		"start": "npm run build && node build/index.js",
		"dev": "nodemon",
		"build": "rm -rf ./build && tsc",
		"lint": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix",
		"lint-staged": "npm run lint",
		"prettier:write": "npx prettier --write **/\"*.{js,jsx,ts,tsx,json}\" && npx prettier --write \"*.{js,jsx,ts,tsx,json}\"",
		"prepare": "husky install"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vedran-omeragic/node.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/vedran-omeragic/node/issues"
	},
	"homepage": "https://github.com/vedran-omeragic/node#readme",
	"dependencies": {
		"@prisma/client": "^4.6.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.14",
		"@types/node": "^18.11.9",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.43.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.2",
		"lint-staged": "^13.0.3",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"prisma": "^4.6.1",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.8.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "sh hooks/pre-commit"
		}
	}
}
